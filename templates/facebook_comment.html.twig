<script>window.fbAsyncInit=function(){FB.init({appId:'{{ applicationCode }}',xfbml:true,version:'v2.5'});};</script>
<p>
  {{ applicationCode }}<br>
  {{ pageUrl }}<br>
  {{ numPosts }}<br>
  {{ width }}<br>
  {{ orderBy }}<br>
  {{ colorScheme }}<br>
</p>
<div class="fb-comments"
  data-href="{{ pageUrl }}"
  data-numposts="{{ numPosts }}"
  data-width="{{ width }}"
  data-order-by="{{ orderBy }}"
  data-colorscheme="{{ colorScheme }}"
  >
</div>
